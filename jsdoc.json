{
	"tags": {
		"allowUnknownTags": false
	},
	"plugins": [
		"plugins/markdown"
	],
	"source": {
		"include": [
			"dist",
			"README.md"
		]
	},
	"opts": {
		"template": "node_modules/docdash",
		"encoding": "utf8",
		"destination": "docs/",
		"recurse": true,
		"verbose": true
	},
	"markdown": {
		"parser": "gfm"
	},
	"docdash": {
		"openGraph": {
			"title": "Jsoning",
			"type": "website",
			"image": "",
			"site_name": "Jsoning",
			"url": "https://jsoning.js.org"
		}
	},
	"meta": {
		"title": "Jsoning",
		"description": "A simple key-value JSON-based persistent lightweight database.",
		"charset": "utf-8"
	},
	"menu": {
		"GitHub": {
			"href": "https://github.com/khalby786/jsoning",
			"target": "_blank",
			"class": "menu-item",
			"id": "github-link"
		}
	}
}