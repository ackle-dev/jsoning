{
	"name": "jsoning",
	"version": "0.14.0",
	"description": "A simple key-value JSON-based persistent lightweight database.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "npm run fmt && tsc && npm run gendocs",
		"delete": "rm tests*/*.test.json",
		"gendocs": "node docma.js && bash scripts/createcname.sh",
		"fmt": "prettier -w src",
		"lint": "eslint src/index.ts",
		"prepack": "npm run build",
		"prepublishOnly": "npm run build",
		"pretest": "npm run build",
		"test": "npx ava tests/test.js",
		"upgrade": "ncu -u && npm i"
	},
	"dependencies": {
		"write-file-atomic": "^5.0.1"
	},
	"devDependencies": {
		"@types/node": "^20.11.30",
		"@types/write-file-atomic": "^4.0.3",
		"@typescript-eslint/eslint-plugin": "^7.3.1",
		"@typescript-eslint/parser": "^7.3.1",
		"all-contributors-cli": "^6.26.1",
		"ava": "^6.1.2",
		"docdash": "^2.0.2",
		"eslint": "^8.57.0",
		"jsdoc": "^4.0.2",
		"npm-check-updates": "^16.14.17",
		"prettier": "3.2.5",
		"typescript": "^5.4.3"
	},
	"repository": "khalby786/jsoning",
	"homepage": "https://jsoning.js.org",
	"license": "MIT",
	"keywords": [
		"json",
		"database"
	],
	"author": {
		"name": "Khaleel Gibran",
		"url": "https://github.com/khalby786"
	},
	"contributors": [
		{
			"name": "Akhil Pillai",
			"url": "https://akpi.is-a.dev/"
		}
	],
	"type": "module"
}
